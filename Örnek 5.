#include <stdio.h>
#include <stdlib.h>



void atama_yap_1(int x) 
{
    x = 20;
}
void atama_yap_2(int *x) 
{
    *x = 30;
}
int main() 
{
    int a = 5;
    
    printf("a: %d\n", a);
    atama_yap_1(a);
    
    printf("a: %d\n", a);
    atama_yap_2(&a);
    
    printf("a: %d\n", a);
return 0;
}
